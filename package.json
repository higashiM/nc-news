  "bugs": {
    "url": "https://github.com/northcoders/be-nc-news/issues"
  },
  "homepage": "https://github.com/northcoders/be-nc-news#readme",
  "dependencies": {
    "apollo-server-express": "^1.4.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai-sorted": "^0.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.7",
    "jsdoc": "^3.6.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.15",
    "moment": "^2.24.0",
    "pg": "^7.18.2",
    "superagent-defaults": "^0.1.14"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "supertest": "^4.0.2"
  }
